{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "artifactName": {
      "type": "string",
      "title": "Long name of the artifact"
    },
    "artifactId": {
      "type": "string",
      "title": "Id of the artifact; name of a .jar file and dockerimage"
    },
    "groupId": {
      "type": "string",
      "title": "groupId"
    },
    "artifactVersion": {
      "type": "string",
      "title": "version"
    },
    "account": {
      "type": "string",
      "title": "Rhizomind platrequired account name"
    },
    "space": {
      "type": "string",
      "title": "Rhizomind platrequired space name"
    },
    "description": {
      "type": "string",
      "title": "Description (this is what's going into package.json)"
    }
  },
  "required": [
    "artifactName",
    "artifactId",
    "groupId",
    "artifactVersion",
    "account",
    "space",
    "description"
  ]
}
