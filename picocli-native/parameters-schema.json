{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "artifactName": {
      "type": "string",
      "title": "Long name of the artifact"
    },
    "artifactId": {
      "type": "string",
      "title": "Id of the artifact; name of a .jar file and dockerimage"
    },
    "groupId": {
      "type": "string",
      "title": "groupId"
    },
    "artifactVersion": {
      "type": "string",
      "title": "version"
    },
    "java-package": {
      "type": "string",
      "title": "package"
    }
  },
  "required": [
    "artifactName",
    "artifactId",
    "groupId",
    "artifactVersion",
    "java-package"
  ]
}
