{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "artifactId": {
      "type": "string",
      "title": "ArtifactId (name of a .jar file and dockerimage)"
    },
    "groupId": {
      "type": "string",
      "title": "Artifact GroupId"
    },
    "artifactName": {
      "type": "string",
      "title": "Long name of the artifact"
    },
    "artifactVersion": {
      "type": "string",
      "title": "version"
    },
    "account": {
      "type": "string",
      "title": "Rhizomind platform account name"
    },
    "space": {
      "type": "string",
      "title": "Rhizomind platform space name"
    },
    "java-package": {
      "type": "string",
      "title": "package"
    }
  },
  "required": [
    "artifactName",
    "artifactId",
    "groupId",
    "artifactVersion",
    "account",
    "space",
    "java-package"
  ]
}
